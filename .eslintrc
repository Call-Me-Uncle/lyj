{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "no-param-reassign": [0],
    "no-underscore-dangle": [0],
    "camelcase": [0],
    "import/no-dynamic-require": [0],
    "prefer-destructuring": [0],
    "no-trailing-spaces": ["error", { "skipBlankLines": true }],
     "object-curly-newline": [0]
  }
}
